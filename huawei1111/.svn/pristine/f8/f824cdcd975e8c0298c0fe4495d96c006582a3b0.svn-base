.checkedAll{
    width: 1200px;
    height: 40px;
    background: #fff;
    color: #a4a4a4;
    margin-bottom: 10px;
}
.checkedAll p{
    height: 40px;
    width: 145px;
    float: left;
    padding-left: 40px;
    line-height: 40px;
    
}
.checkedAll p span{
    font-size: 12px;
    margin-right: 10px;
}
.checkedAll ul{
    float: left;
}
.checkedAll ul li{
    float: left;
    height: 40px;
    line-height: 40px;
}
.checkedAll ul li{
    width: 125px;
    text-align: center;
}
.checkedAll ul li:first-child{
    width: 475px;
    text-align: left;
}
